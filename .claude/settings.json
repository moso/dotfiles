{
    "$schema": "https://json.schemastore.org/claude-code-settings.json",
    "env": {
        "CLAUDE_CODE_ENABLE_TELEMETRY": "0",
        "DISABLE_ERROR_REPORTING": "1",
        "DISABLE_TELEMETRY": "1"
    },
    "forceLoginMethod": "claudeai",
    "includeCoAuthoredBy": false,
    "learnMode": false,
    "model": "claude-opus-4-1-20250805",
    "permissions": {
        "deny": [
            "Bash(git commit)",
            "Bash(git fetch)",
            "Bash(git pull)",
            "Bash(git push)",
            "Read(./.env)",
            "Read(./env.*)",
            "Read(./build)",
            "Read(./dist)"
        ]
    }
}
